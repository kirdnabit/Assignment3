public class RegularBox extends Box {
  private String items;
  private double weight;

  public RegularBox(){
    super();
    weight = 0;
    items = "";
  }

  public RegularBox(String dA, String rA, double w, double l, double h, int count, String i, double wt){
    super(dA, rA, w, l, h, count);
    items = i;
    weight = wt;
  }
  
  public String getItems(){
    return items;
  }
  
  public double getWeight(){
    return weight;
  }
  
  @Override
  public String toString(){
    return getId() + "\n" + getDeliveryAddress() + "\n" + getReturnAddress()
    + "\n" + getWidth() + "x" + getLength() + "x" + getHeight() 
    + "\nItems: " + getItems() + "\nCount: " + getCount() + "\nWeight: " + getWeight();
  }
  
}
